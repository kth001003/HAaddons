{
  "name": "[TEST] COMMAX Wallpad",
  "version": "1.2.5b",
  "slug": "commax",
  "url": "https://github.com/wooooooooooook/HAaddons",
  "description": "mqtt 통신을 활용한 코맥스 월패드 컨트롤러",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    "share:rw"
  ],
  "startup": "application",
  "boot": "auto",
  "options": {
    "queue_interval_in_second": "0.1",
    "max_send_count" : 10,
    "DEBUG": false, 
    "mqtt_log": true,
    "elfin_log": false,
    "mqtt_server": "192.168.x.x",
    "mqtt_id": "id",
    "mqtt_password": "password",
    "mqtt_TOPIC": "commax",
    "elfin_auto_reboot": true,
    "elfin_server": "192.168.x.x",
    "elfin_id": "admin",
    "elfin_password": "elfin_password",
    "elfin_reboot_interval": 100
  },
  "schema": {
    "queue_interval_in_second": "float",    
    "max_send_count" : "int",
    "DEBUG": "bool",
    "mqtt_log": "bool",
    "elfin_log": "bool",
    "mqtt_server": "str",
    "mqtt_id": "str",
    "mqtt_password": "password",
    "mqtt_TOPIC": "str",
    "elfin_auto_reboot": "bool",
    "elfin_server": "str?",
    "elfin_id": "str?",
    "elfin_password": "password?",
    "elfin_reboot_interval": "int"
  }
}