{
  "name": "[TEST] COMMAX Wallpad",
  "version": "2.3.0",
  "slug": "cwbs",
  "url": "https://github.com/wooooooooooook/HAaddons",
  "description": "mqtt 통신을 활용한 코맥스 월패드 컨트롤러",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    "share:rw"
  ],
  "startup": "application",
  "boot": "auto",
  "options": {
    "DEBUG": {
      "enabled": false,
      "mqtt_log": true,
      "elfin_log": false
    },
    "mqtt_server": "192.168.x.x",
    "mqtt_id": "id",
    "mqtt_password": "password",
    "mqtt_TOPIC": "cwbs",
    "elfin_auto_reboot": true,
    "elfin_server": "192.168.x.x",
    "elfin_id": "admin",
    "elfin_password": "elfin_password",
    "elfin_reboot_interval": 100
  },
  "schema": {
    "DEBUG": {
      "type": "dict",
      "keys": {
        "enabled": {
          "type": "bool",
          "description": "Set log level to DEBUG"
        },
        "mqtt_log": {
          "type": "bool",
          "description": "Show MQTT message logs from HA"
        },
        "elfin_log": {
          "type": "bool",
          "description": "Show RS485 packet logs from EW11"
        }
      }
    },
    "mqtt_server": {
      "type": "str"
    },
    "mqtt_id": {
      "type": "str"
    },
    "mqtt_password": {
      "type": "str"
    },
    "mqtt_TOPIC": {
      "type": "str"
    },
    "elfin_auto_reboot": "bool",
    "elfin_server": "str?",
    "elfin_id": "str?",
    "elfin_password": "str?",
    "elfin_reboot_interval": "int?"
  }
}
