{
  "name": "[alpha]heatmapbuilder",
  "version": "0.1.1",
  "slug": "heatmapbuilder",
  "url": "https://github.com/wooooooooooook/HAaddons",
  "description": "개발용입니다. 절대 메인으로 사용하지 마세요.",
  "arch": ["armhf", "armv7", "aarch64", "amd64", "i386"],
  "map": [
    {
      "type": "homeassistant_config",
      "read_only": false,
      "path": "/homeassistant"
    }
  ],
  "startup": "application",
  "boot": "auto",
  "init": false,
  "hassio_api": true,
  "auth_api": true,
  "homeassistant_api": true,
  "options": {
    "log_level": "info"
  },
  "schema": {
    "log_level": "list(debug|info|warning|error|fatal)"
  },
  "ingress": true,
  "ingress_port": 8099,
  "ports": {
    "8099/tcp": "for Web UI",
    "8554/tcp": "for RTSP Stream"
  }
}